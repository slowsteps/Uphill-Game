package  {		import flash.display.MovieClip;	import flash.events.Event;			public class BoostEffect extends MovieClip {						public function BoostEffect() {			// constructor code			this.addEventListener(Event.ENTER_FRAME,EnterFrame);					}				public function setStrength(strength:int) {			scaleX = strength;			alpha = 1;		}				private function EnterFrame(e:Event) {		  alpha = alpha - 0.05;		}			}	}