package  {	import flash.display.MovieClip	import flash.net.SharedObject;	import flash.events.Event;	import flash.events.MouseEvent;	import flash.events.KeyboardEvent;		import com.greensock.TweenLite;	//import com.greensock.*;	import com.greensock.easing.*	import com.google.analytics.AnalyticsTracker;	import com.google.analytics.GATracker	public class Ending extends MovieClip{		private var myLoader:GameLoader		public function Ending(inLoader:GameLoader) {			trace(this,this.name,Math.random())			myLoader = inLoader			visible = false			nextbutton.visible=false		}				public function showResult() {			visible = true			var cookie = SharedObject.getLocal("GameState")			var days = cookie.data.attempts			shirt.textfield.text = "I crossed the portal in " + days + " days"						showGifts()			myLoader.tracker.trackEvent("motorabbit","gamestate","ending")					}				private function showGifts() {			TweenLite.from(photo, 1, {y:-500,ease:Bounce.easeOut});			TweenLite.from(shirt, 1, {y:-500,ease:Bounce.easeOut,delay:2,onComplete:onComplete});		}				private function onNext(e:Event) {						myLoader.resetFromEnding()			this.hide()		}		private function onComplete() {			nextbutton.visible = true			nextbutton.addEventListener(MouseEvent.CLICK,onNext)		}		public function hide() {			this.visible = false		}	}	}