package  {	import flash.display.MovieClip	import flash.net.SharedObject;	import flash.events.Event;	import flash.events.MouseEvent;	import flash.events.KeyboardEvent;		import com.greensock.TweenMax;	import com.greensock.*;	import com.greensock.easing.*;	public class Ending extends MovieClip{		private var myLoader:GameLoader		public function Ending(inLoader:GameLoader) {			trace(this,this.name,Math.random())			myLoader = inLoader			visible = false		}				public function showResult() {			visible = true			var cookie = SharedObject.getLocal("GameState")			var days = cookie.data.attempts			shirt.textfield.text = "I crossed the portal in " + days + " days"			nextbutton.addEventListener(MouseEvent.CLICK,onNext)			showGifts()					}				private function showGifts() {			TweenMax.from(photo, 1, {y:-500,ease:Bounce.easeOut});			TweenMax.from(shirt, 1, {y:-500,ease:Bounce.easeOut,delay:2});		}				private function onNext(e:Event) {						myLoader.resetFromEnding()			this.hide()		}		public function hide() {			this.visible = false		}	}	}