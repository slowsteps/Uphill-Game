package  {		import flash.display.MovieClip;	import flash.events.Event;	import flash.events.Event;	import flash.events.MouseEvent;			public class Intro extends MovieClip {			//private var start:MovieClip							public function Intro() {			alpha = 0;			addEventListener(Event.ENTER_FRAME,onEnterFrame);					}				private function onEnterFrame(e:Event) {			alpha+=0.02;		}						}	}