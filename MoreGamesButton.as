package  {	import flash.display.MovieClip	import flash.text.TextField;	import flash.net.navigateToURL	import flash.events.Event	import flash.events.MouseEvent	import flash.net.URLRequest;	public class MoreGamesButton extends ButtonEffects {		public function MoreGamesButton() {			super()			addEventListener(MouseEvent.CLICK,onClick)		}		private function onClick(e:Event) {						var req:URLRequest = new URLRequest("http://www.girlwithgun.com")			navigateToURL(req,"_blank")		}	}	}