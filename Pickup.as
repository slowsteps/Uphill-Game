package  {		import flash.display.MovieClip;	import fl.transitions.Tween;	import fl.transitions.easing.*;			public class Pickup extends MovieClip {				public var collected:Boolean;		public var main:Main;				public function Pickup() {			collected = false;			//flyout anim			 		}						public function Collect() {			collected = true; //to prevent multiple collisions			visible = false;			//var aPickupEffect = new PickupEffect();			var aPickupEffect = new PickupEffect();			aPickupEffect.main = main;			aPickupEffect = stage.addChild(aPickupEffect);			aPickupEffect.startAnimation(this);		}			}	}