package  {		import flash.display.MovieClip;	import fl.transitions.Tween;	import fl.transitions.easing.*;			public class Pickup extends MovieClip {				public var collected:Boolean;		public var myMain:Main;		public var startx:Number;				public function Pickup(inMain:Main) {			myMain=inMain			collected = false;			//flyout anim			 		}				public function setPosition(xpos,ypos) {			startx = xpos;			x = xpos;			y = ypos;		}				public function Collect() {						collected = true; //to prevent multiple collisions						visible = false;			myMain.addFuel(50)			/*			var aPickupEffect = new PickupEffect(myMain);			aPickupEffect = stage.addChild(aPickupEffect);			aPickupEffect.startAnimation(this);			*/		}			}	}