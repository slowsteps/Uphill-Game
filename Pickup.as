package  {		import flash.display.MovieClip;	import com.greensock.TweenMax;	import com.greensock.*;	import com.greensock.easing.*;			public class Pickup extends MovieClip {				public var collected:Boolean;		public var myMain:Main;		public var startx:Number;				public function Pickup(inMain:Main) {			myMain=inMain			collected = false;			//flyout anim			 		}				public function setPosition(xpos,ypos) {			startx = xpos;			x = xpos;			y = ypos;		}				public function Collect() {						collected = true; //to prevent multiple collisions			TweenMax.to(this, 0.6, {y:"-600",onComplete:showComplete});					}				private function showComplete() {			visible = false;			myMain.addFuel(50)		}			}	}