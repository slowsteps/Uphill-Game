package  {		import flash.display.MovieClip;	import fl.transitions.Tween;	import fl.transitions.easing.*;	import fl.transitions.TweenEvent;	import flash.geom.Point;			public class PickupEffect extends MovieClip {				private var myMain:Main;		var xtween:Tween;		var ytween:Tween;				public function PickupEffect(inMain:Main) {			myMain = inMain			trace("pickup effect",this,myMain)					}				public function startAnimation(spawn:Pickup) {					var myPoint = new Point();			myPoint.x = spawn.x;			myPoint.y = spawn.y;			myPoint = spawn.parent.localToGlobal(myPoint);						xtween = new Tween(this, "x", Regular.easeInOut, myPoint.x, 50, 1, true);			ytween = new Tween(this, "y", Regular.easeInOut, myPoint.y, 50, 1, true);						xtween.addEventListener(TweenEvent.MOTION_FINISH,onTweenFinished);		}				private function onTweenFinished(e:TweenEvent = null) {			trace("Pickup effect, finish tween")			visible = false;			myMain.addFuel(200);		}			}	}